
<%= form_for @game do |f| %>
  
  <%= f.label :player_one_name %>
  <%= f.text_field :player_id %><br />

  <%= f.label :player_two_name %>
  <%= f.text_field :opponent_id %><br />

  <%= f.label :player_one_score %>
  <%= f.text_field :player_score %><br />

  <%= f.label :player_two_score %>
  <%= f.text_field :opponent_score %><br />

  <%= f.submit %>

<% end %>